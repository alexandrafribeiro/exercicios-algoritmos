algoritmo "semnome"

var
     Cont, nota, T: Real
     Nome: caractere
inicio
      Cont <- 1
      Escreva("Quantos alunos a turma tem? ")
      Leia (T)
      Enquanto (Cont <= T) faca
               Escreva ("Nome do aluno: ")
               Leia (nome)
               Escreva ("Nota do aluno: ")
               Leia (nota)
      FimEnquanto
fimalgoritmo
